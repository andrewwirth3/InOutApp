{
    "name": "in-out",
    "version": "1.0.0",
    "description": "A native application built with NativeScript-Vue",
    "author": "Andrew Wirth <andrewwirth3@gmail.com>",
    "license": "MIT",
    "nativescript": {
        "id": "in.out.andrew.wirth",
        "tns-android": {
            "version": "5.3.1"
        },
        "tns-ios": {
            "version": "5.3.1"
        }
    },
    "scripts": {
        "build": "tns build android --bundle",
        "build.prod": "tns build android --bundle --env.production",
        "run": "tns run android --bundle",
        "run.prod": "tns run android --bundle --env.production",
        "preview": "tns preview android --bundle",
        "clean": "tns platform remove android",
        "debug": "tns debug android --bundle",
        "clean.build": "npm run clean && npm run build",
        "clean.build.run": "npm run clean && npm run build && npm run run",
        "clean.build.debug": "npm run clean && npm run build && npm run debug",
        "lint": "tslint --project tsconfig.json"
    },
    "dependencies": {
        "@vue/devtools": "^5.0.6",
        "axios": "^0.21.1",
        "build-url": "^1.3.2",
        "nativescript-fonticon": "^2.0.0",
        "nativescript-socketio": "^3.2.1",
        "nativescript-theme-core": "^1.0.4",
        "nativescript-toasty": "^1.3.0",
        "nativescript-ui-autocomplete": "^4.0.0",
        "nativescript-ui-calendar": "^4.0.0",
        "nativescript-ui-dataform": "^4.0.0",
        "nativescript-ui-listview": "^6.2.0",
        "nativescript-ui-sidedrawer": "^4.3.0",
        "nativescript-vue": "^2.0.0",
        "nativescript-vue-devtools": "^1.2.0",
        "tns-core-modules": "^5.0.2",
        "vuex": "^3.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "babel-loader": "^8.0.2",
        "babel-traverse": "6.26.0",
        "babel-types": "6.26.0",
        "babylon": "6.18.0",
        "clean-webpack-plugin": "^0.1.19",
        "copy-webpack-plugin": "^4.5.2",
        "css-loader": "^1.0.0",
        "lazy": "1.0.11",
        "nativescript-dev-webpack": "^0.24.1",
        "nativescript-vue-template-compiler": "^2.0.0",
        "nativescript-worker-loader": "~0.9.0",
        "node-sass": "^4.9.2",
        "sass-loader": "^7.1.0",
        "terser-webpack-plugin": "^1.1.0",
        "tns-platform-declarations": "^5.1.2",
        "typescript": "^3.2.4",
        "vue": "^2.5.22",
        "vue-loader": "^15.2.6",
        "webpack": "^4.16.4",
        "webpack-bundle-analyzer": "^3.3.2",
        "webpack-cli": "^3.1.0"
    }
